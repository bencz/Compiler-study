type Sequence = array [1..10] of integer;

var M,N : integer;
    X   : Sequence;

procedure Generate(k: integer);
var i,j : integer;
begin   
    if k=N then
    begin
        for i:=1 to N do
            write(X[i]);writeln
    end
    else
        for j:=1 to M do
        begin
            X[k+1]:=j;
            Generate(k+1)
        end
end;

begin
    m:=2;
    n:=3;
    Generate(0)
end.

